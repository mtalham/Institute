schema {
	query: Query
	mutation: Mutation
}

type Query {
allStudents: [Student!]
}

type Mutation {
createStudent(first: String, last: String, email: String): Student!
modifyStudent(first: String!, last: String!, email: String!, uuid: String!): Student!
}

type Student {
	firstName: String!
	lastName: String!
	email: String!
	uuid: String!
}